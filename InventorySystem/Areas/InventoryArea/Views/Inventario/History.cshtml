
@{
    ViewData["Title"] = "History";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="container pt-5">

    <div class="row">
        <div class="col-6">
            <h2 class="text-primary">Historial Inventariado</h2>
        </div>
    </div>

    <div class="p-4 border rounded">
        <table id="tblDatos" class="table table-striped table-bordered w-100">
            <thead>
                <tr class="table-info ">
                    <th class="text-dark">
                        Fecha inicio
                    </th>
                    <th class="text-dark">
                        Fecha Fin
                    </th>
                    <th class="text-dark">
                        Bodega
                    </th>
                    <th class="text-dark">
                        Usuario
                    </th>
                    <th class="text-dark">
                        Acción
                    </th>
                </tr>
            </thead>
        </table>
    </div> 

    <div class="col-6 p-2">
        <a class="btn btn-info" asp-action="Index">
            <i class="fa fa-chevron-circle-left"></i> Volver
        </a>
    </div>
</div>

@section Scripts{ 
    <script src="~/js/HistorialScripts.js"></script>
}